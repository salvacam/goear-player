<?xml version='1.0' encoding='utf-8'?>
<widget id="mx.x10.salvacam.goear" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Goear Player</name>
    <description>
        Reproductor goear
    </description>
    <author email="salvacams@gmail.com" href="https://github.com/salvacam">
        Salvador Camacho
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="de.appplant.cordova.plugin.local-notification" />
    <plugin name="cordova-plugin-dialogs" />
    <plugin name="org.apache.cordova.network-information" />
    <plugin name="org.apache.cordova.file" />
    <plugin name="org.apache.cordova.file-transfer" />
    <!-- <plugin name="nl.x-services.plugins.headsetdetection" /> -->
    
    
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
